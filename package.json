{
  "name": "swedenbank-webpack",
  "version": "1.0.0",
  "description": "webComponents VanillaJS",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/public/index.html --out-dir development -p 3000",
    "build": "rimraf ./dist && parcel build src/public/index.html --out-dir dist --public-url ./",
    "clean": "rimraf ./development && rimraf ./.cache"
  },
  "keywords": [
    "webComponents",
    "VanillaJS"
  ],
  "author": "Nadezhda Barinova",
  "license": "ISC",
  "dependencies": {},
  "alias": {
    "assets": "./src/public/assets/",
    "styles": "./src/style/"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-inlinesvg": "^0.1.1",
    "rimraf": "^3.0.2"
  }
}
